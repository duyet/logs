{
  "name": "cloudflare-analytics-router",
  "version": "1.0.0",
  "description": "Analytics data router for Cloudflare Pages with Hono",
  "type": "module",
  "scripts": {
    "dev": "wrangler pages dev --compatibility-date=2024-01-01 -- npm run build:watch",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "deploy": "npm run build && npm test && wrangler pages deploy",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.5.0",
    "@cloudflare/workers-types": "^4.20240117.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0",
    "wrangler": "^3.25.0"
  }
}
